<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="750.0" prefWidth="1000.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="puzzle.view.MainMenuController">
  <center>
    <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
      <children>
        <Button mnemonicParsing="false" onAction="#openPegPuzzle" prefHeight="85.0" prefWidth="200.0" text="Puzzle 1">
          <font>
            <Font size="24.0" fx:id="x1" />
          </font>
          <VBox.margin>
            <Insets top="50.0" />
          </VBox.margin>
        </Button>
        <Button font="$x1" mnemonicParsing="false" onAction="#openSoccerBox" prefHeight="85.0" prefWidth="200.0" text="SoccerBox">
          <VBox.margin>
            <Insets top="30.0" />
          </VBox.margin>
        </Button>
        <Button font="$x1" mnemonicParsing="false" prefHeight="85.0" prefWidth="200.0" text="Button">
          <VBox.margin>
            <Insets top="30.0" />
          </VBox.margin>
        </Button>
      </children>
    </VBox>
  </center>
  <top>
    <Label text="Main Menu" BorderPane.alignment="CENTER">
      <font>
        <Font size="36.0" />
      </font>
      <BorderPane.margin>
        <Insets top="15.0" />
      </BorderPane.margin>
    </Label>
  </top>
</BorderPane>
